
<%%= tb_fixed_toolbar("<%= plural_model_name.humanize %>", <%= namespace_path %><%= plural_resource_name %>_path) do %>
  <%%= tb_submit_button_to @<%= resource_name  %>%>
  <%%= tb_cancel_button_to @<%= resource_name %> %>
<%% end %>


<%%= simple_form_for @<%= resource_name %>, :url => <%= namespace_path %><%= plural_resource_name %>_path,:html => { :class => 'form-vertical', :id => '<%= resource_name %>' } do |f| %>
  
  <%%= render :partial=>'form', :locals=>{:f=>f} %>

<%%end%>
